  {
      "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
      "data": {
        "url": "https://raw.githubusercontent.com/vsur0007/FIT_3179_Assignment_2/main/Earthquake_Data.csv"
      },
      "title": "Number of Earthquake occurrences in USA",
      "mark": {
        "type": "bar",
        "tooltip": true
      },
      "encoding": {
        "x": {
          "field": "mag",
          "bin": {"maxbins": 20},
          "title": "Magnitude"
        },
        "y": {
          "aggregate": "count",
          "title": "Count of Earthquakes"
        }
      },
      "width": 600,
      "height": 400,
      "config": {"title": {"font": "sans-serif", "fontSize": 16}}
  }
    
